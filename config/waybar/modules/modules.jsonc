{
  // 屏幕亮度状态
  "backlight": {
    // 轮询信息的时间间隔
    // "interval": 2,
    // 格式化，屏幕亮度百分比信息显示
    "format": "{icon}",
    // "format": "{icon} {percent}%",
    // 根据当前屏幕亮度亮度百分比，选择相应的图标，顺序从低到高
    "format-icons": ["", "", "", "", "", "", "", "", ""],
    // 模块应显示的最小长度（以字符为单位）。
    // "min-length": 2,
    // 模块应显示的最大长度（以字符为单位）。
    // "max-length": 7,
    // 文本旋转度
    // "rotate": 0,
    // 鼠标左键点击
    // "on-click": "",
    // 鼠标滚轮点击
    // "on-click-middle": "",
    // 鼠标右键点击
    // "on-click-right": "",
    // 鼠标滚轮向上滚动
    // "on-scroll-up": "brightnessctl -n set 1%+",
    // "on-scroll-up": "",
    // 鼠标滚轮向下滚动
    // "on-scroll-down": "brightnessctl -n set 1%-",
    // "on-scroll-down": "",
    // 滚动时更改亮度的速度。
    "scroll-step": 2,
    // 悬停时显示提示
    "tooltip": true,
    // 悬停时显示提示的文本
    "tooltip-format": "滚动鼠标滚轮以调整亮度\n当前屏幕亮度：{percent}%",
  },
  // 电池状态
  "battery": {
    // 要监控的电池，如/sys/class/power_supply/内选择，而不是自动检测
    // "bat": BAT2,
    // 要监控的适配器，如/sys/class/power_supply/内选择，而不是自动检测
    // "adapter": BAT2,
    // 启用使用设计容量而不是实际最大容量的选项。因此，即使充满，电池也可能低于 100%。
    // "design-capacity": false,
    // 定义电池的最大百分比，对旧电池有用
    // "full-at": 100,
    // 轮询信息的时间间隔
    "interval": 20,
    // 格式化，电池余量百分比信息
    "format": "{icon}",
    // "format": "{icon} {capacity}%",
    // 格式化，电池预计可使用时间
    "format-time": "{H} hr {M} min",
    // 根据当前电池余量百分比，选择相应的图标，顺序从低到高
    "format-icons": {
      "charging": ["󰢜", "󰂆", "󰂇", "󰂈", "󰢝", "󰂉", "󰢞", "󰂊", "󰂋", "󰂅"],
      "default": ["󰁺", "󰁻", "󰁼", "󰁽", "󰁾", "󰁿", "󰂀", "󰂁", "󰂂", "󰁹"],
    },
    // 低电量状态定义，用来处理特殊的电量事件
    "states": {
      // 电池使用剩下30%容量
      "warning": 30,
      // 电池使用剩下10%容量
      "critical": 10,
    },
    // 模块应显示的最小长度（以字符为单位）。
    // "min-length": 2,
    // 模块应显示的最大长度（以字符为单位）。
    // "max-length": 7,
    // 文本旋转度
    // "rotate": 0,
    // 鼠标左键点击
    // "on-click": "",
    // 鼠标滚轮点击
    // "on-click-middle": "",
    // 鼠标右键点击
    // "on-click-right": "",
    // 鼠标滚轮向上滚动
    // "on-scroll-up": "",
    // 鼠标滚轮向下滚动
    // "on-scroll-down":  "",
    // 滚动时要使用的阈值。
    // "smooth-scrolling-threshold": "",
    // 悬停时显示提示
    // "tooltip": true,
    // 悬停时显示充电提示的文本
    // "tooltip-format-charging": "电池预计充电需要时间: {time}\n当前电量：{capacity}%",
    // "tooltip-format-discharging": "电池剩余可用时间：{time}\n当前剩余电量：{capacity}%",
    "tooltip-format-charging": "{power:>1.0f}W↑ {capacity}%",
    "tooltip-format-discharging": "{power:>1.0f}W↓ {capacity}%",
    // 对于具有多个电池的设备，可以选择将百分比显示为按电池大小加权的平均值，而不是其百分比级别的简单平均值
    // "weighted-average": false,
    // 如果未检测到电池兼容性，则可选择启用电池兼容性。
    // "bat-compatibility": false,
    // 电池电量百分比事件
    "events": {
      "on-discharging-warning": "notify-send '低电量提示: 已不足30%电量' -t 5000",
      "on-discharging-critical": "notify-send '低电量提示: 已不足10%电量' -u critical -t 5000",
      // "on-charging-100": "notify-send '电池已充满' -t 5000"
    },
  },
  // 蓝牙状态
  "bluetooth": {
    // 使用配置的蓝牙连接，否则，将随机使用可用蓝牙适配器。建议有多个蓝牙适配器时配置默认连接外设的蓝牙（基本没有人一台有多个可连接蓝牙适配器）。
    // "controller": "",
    // "format-device-preference": ,
    // 格式化，蓝牙状态
    "format": "󰂯",
    // 格式化，蓝牙不可用状态
    "format-disabled": "󰂲",
    // 格式化，蓝牙禁用状态
    "format-off": "󰂲",
    // 格式化，蓝牙启用状态
    "format-on": "󰂰",
    // 格式化，蓝牙已连接最后一台设备状态
    "format-connected": "󰂱",
    // 格式化，已连接蓝牙电池信息
    // "format-connected-battery": "",
    // 格式化，无可用蓝牙适配器信息
    // "format-no-controller": "",
    // 模块应显示的最小长度（以字符为单位）。
    // "min-length": 2,
    // 模块应显示的最大长度（以字符为单位）。
    // "max-length": 2,
    // 文本旋转度
    // "rotate": ,
    // 鼠标左键点击
    "on-click": "bluetoothctl power on && notify-send '蓝牙已启用'",
    // 鼠标左键双击
    "on-double-click": "blueman-manager",
    // "on-double-click": "$TERMINAL ",
    // 鼠标滚轮点击
    // "on-click-middle": "",
    // 鼠标右键点击
    // "on-click-right": "",
    "on-click-right": "bluetoothctl power off && notify-send '蓝牙已禁用' -r 1925",
    // 鼠标滚轮向上滚动
    // "on-scroll-up": "",
    // 鼠标滚轮向下滚动
    // "on-scroll-down":  "",
    // 滚动时要使用的阈值。
    // "smooth-scrolling-threshold": "",
    // 悬停时显示提示
    // "tooltip": true,
    // "tooltip-format": "蓝牙设备地址: {device_address}",
    "tooltip-format": "已连接蓝牙设备数量：{num_connections}",
    // "tooltip-format-disabled": "蓝牙不可用",
    "tooltip-format-off": "蓝牙已关闭",
    "tooltip-format-on": "蓝牙已开启",
    "tooltip-format-connected": "蓝牙最近连接设备名称: {device_alias}",
    "tooltip-format-enumerate-connected": "{device_alias}",
    "tooltip-format-connected-battery": "{device_alias} 电量: {device_battery_percentage}%",
    "tooltip-format-enumerate-connected-battery": "{device_alias} 电量: {device_battery_percentage}%",
  },
  // cpu状态
  "cpu": {
    // 轮询信息的时间间隔
    "interval": 5,
    // 格式化，CPU利用率百分比信息
    "format": "󰍛 {usage}%",
    // 格式化，CPU利用率警告百分比信息
    "format-warning": "󰀨 {usage}%",
    // 格式化，CPU利用率警告百分比信息
    "format-critical": "󰀨 {usage}%",
    "states": {
      // CPU使用率达到75%状态
      "warning": 75,
      // CPU使用率达到90%状态
      "critical": 90,
    },
    // 模块应显示的最小长度（以字符为单位）。
    // "min-length": 7,
    // 模块应显示的最大长度（以字符为单位）。
    // "max-length": 7,
    // 文本旋转度
    // "rotate": 0,
    // 鼠标左键点击
    "on-click": "$TERMINAL -T htop sh -c 'htop -s PERCENT_CPU'",
    // 鼠标滚轮点击
    // "on-click-middle": "",
    // 鼠标右键点击
    // "on-click-right": "",
    // 鼠标滚轮向上滚动
    // "on-scroll-up": "",
    // 鼠标滚轮向下滚动
    // "on-scroll-down":  "",
    // 滚动时要使用的阈值。
    // "smooth-scrolling-threshold": "",
    // 悬停时显示提示
    // "tooltip": false,
    // 悬停时显示提示的文本
    "tooltip-format": "平均睿频：{avg_frequency}GHz\n最小睿频：{min_frequency}\n最大睿频{max_frequency}",
  },
  // cava（跨平台条形音乐音谱条可视化工具）
  // "cava": {},
  // 时间状态
  "clock": {
    // 轮询信息的时间间隔
    "interval": 60,
    // 格式化，时间信息
    "format": "󰸗 {:%m月%d日 %H:%M}",
    // 时区
    // "timezone": "",
    // 多时区配置，与时区timezone冲突
    // "timezone": [""],
    // 自定义时区显示的语言
    // "locate": "",
    // 文本旋转度
    // "rotate": 0,
    // 鼠标左键点击
    // "on-click": "",
    // 鼠标滚轮点击
    // "on-click-middle": "",
    // 鼠标右键点击
    // "on-click-right": "",
    // 鼠标滚轮向上滚动
    // "on-scroll-up": "",
    // 鼠标滚轮向下滚动
    // "on-scroll-down":  "",
    // 滚动时要使用的阈值。
    // "smooth-scrolling-threshold": "",
    // 悬停时显示提示
    // "tooltip": false,
    // 悬停时显示提示的文本
    "tooltip-format": "<span size='9pt' font='WenQuanYi Zen Hei Mono'>{calendar}</span>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      // "on-click-right": "mode",
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b><u>{}</u></b></span>",
      },
    },
    "actions": {
      // "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down",
    },
  },
  // hyprland聚焦窗口状态
  "hyprland/windows": {
    // 格式化，聚焦窗口状态信息
    "format": "{}",
    "rewrite": {
      "^$": "Desktop",
      "^$TERMINAL$": "Terminal",
      "^zsh$": "Terminal",
      "^~$": "Terminal",
    },
    // 显示waybar所属显示器的活动窗口信息，而不是聚焦窗口信息。
    "separate-outputs": true,
    // 是否隐藏窗口图标
    // "icon": false,
    // 窗口图标大小
    "icon-size": 24,
  },
