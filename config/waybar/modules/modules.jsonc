  "hyprland/windowcount": {
    // 格式化。当前工作区窗口数量状态信息
    "format": "[{}]",
    // 格式化。当前工作区无窗口状态信息
    "format-empty": "[X]",
    // 格式化。当前工作区窗口无全屏窗口状态信息
    "format-windowed": "[T]",
    // 格式化。当前工作区窗口全屏窗口状态信息
    // "format-fullscreen": "[{}]",
    // 显示waybar所属显示器的活动窗口信息，而不是聚焦工作区信息。
    "separate-outputs": true,
  },
